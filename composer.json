{
    "name": "evolvo/laravel-code-generators",
    "description": "Artisan commands to generate laravel project code from existing database tables.",
    "type": "library",
    "require": {
        "laravel/framework": "^5.5 | ^6.0",
        "php": ">=7.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aivaras",
            "email": "aivaras@evolvo.lt"
        }
    ],
	"autoload": {
        "psr-4": {
            "Evolvo\\LaravelCodeGenerators\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Evolvo\\LaravelCodeGenerators\\LaravelCodeGeneratorsServiceProvider"
            ]
        }
},
    "minimum-stability": "stable"
}
